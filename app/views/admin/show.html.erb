<h1>Admin Dashboard</h1>

<div class="table-responsive">
  <table class="table">
    <thead>
      <tr>
        <th>Order</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <%= @orders.each do |order| %>
      <tr>
        <td><%= link_to order.id, order_path(order) %></td>
        <td><%= order.status %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
